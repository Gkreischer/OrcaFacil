{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../node_modules/loopback-ds-timestamp-mixin",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db"
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "MongoDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "MongoDB",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "MongoDB",
    "public": false
  },
  "peca": {
    "dataSource": "MongoDB",
    "public": true
  },
  "categorias": {
    "dataSource": "MongoDB",
    "public": true
  },
  "pecasPed": {
    "dataSource": "MongoDB",
    "public": true
  },
  "infoempresa": {
    "dataSource": "MongoDB",
    "public": true
  },
  "configuracao": {
    "dataSource": "MongoDB",
    "public": true
  },
  "historicoPedidos": {
    "dataSource": "MongoDB",
    "public": true
  },
  "container": {
    "dataSource": "storage",
    "public": true
  },
  "orcamentos": {
    "dataSource": "MongoDB",
    "public": true
  }
}
